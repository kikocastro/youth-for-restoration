<html>
<head>
    {% block head -%}
        <title>
            {%- block title -%}
                {%- if record.title is defined %}{{ record.title|striptags }} | {% endif -%}
                    {{ app.config.get('general/sitename') }}
                    {%- if record.title is not defined and app.config.get('general/payoff') %} | {{ app.config.get('general/payoff') }}{% endif -%}
                    {%- endblock title -%}
                </title>
                <meta http-equiv="content-type" content="text/html; charset=utf-8" />
                <meta name="description" content="" />
                <meta name="keywords" content="" />
                <script>
                    var paths = {
                        theme : '{{ paths.theme }}'
                    };
                </script>

                {%- block scripts %}

                    <!--[if lte IE 8]><script src="{{ paths.theme }}css/ie/html5shiv.js"></script><![endif]-->
                    <script src="{{ paths.theme }}js/jquery.min.js"></script>
                    <script src="{{ paths.theme }}js/jquery.dropotron.min.js"></script>
                    <script src="{{ paths.theme }}js/jquery.scrolly.min.js"></script>
                    <script src="{{ paths.theme }}js/jquery.scrollgress.min.js"></script>
                    <script src="{{ paths.theme }}js/skel.min.js"></script>
                    <script src="{{ paths.theme }}js/skel-layers.min.js"></script>
                    <script src="{{ paths.theme }}js/init.js"></script>
                    <script src="{{ paths.theme }}js/lightbox.min.js"></script>

                {% endblock scripts %}

                {%- block stylesheets %}

                    <noscript>
                        <link rel="stylesheet" href="{{ paths.theme }}css/skel.css" />
                        <link rel="stylesheet" href="{{ paths.theme }}css/style.css" />
                        <link rel="stylesheet" href="{{ paths.theme }}css/style-wide.css" />
                        <link rel="stylesheet" href="{{ paths.theme }}css/style-noscript.css" />
                        <link rel="stylesheet" href="{{ paths.theme }}css/searchbox.css" />
                    </noscript>
                    <!--[if lte IE 8]><link rel="stylesheet" href="{{ paths.theme }}css/ie/v8.css" /><![endif]-->
                    <!--[if lte IE 9]><link rel="stylesheet" href="{{ paths.theme }}css/ie/v9.css" /><![endif]-->

                {% endblock stylesheets %}

            {%- endblock head %}

        </head>
        <body class="{% block bodyclass %}{% endblock %}">

            {% block header %}
                <header id="header" class="{% block headerclass %}{% endblock %}">
                    <div class="logo">
                        <a href="{{ paths.root }}" class="no-underline">
                            <img id='yfr-logo' src="{{ app.paths.app }}view/img/logo.png" width="115" height="auto" alt="Love our community and cookies? Come and join us!">
                        </a>
                    </div>
                    <nav id="nav">
                        <ul>
                            {{ menu('main', '_menu.twig') }}
                            <li>
                                <div id="wrap">
                                    <form method="get" action="{{ paths.root }}search" id="searchform" enctype="text/plain" autocomplete="on">
                                        <input id="search" name="q" type="text" placeholder="What are we looking for?">
                                        <input id="search_submit" value="" type="submit"><span class="icon fa-search"></span></input>
                                    </form>
                                </div>
                            </li>
                        </li>
                    </ul>
                </nav>
                <div id="wrap" class="mobile-search">
                    <form method="get" action="{{ paths.root }}search" id="searchform" enctype="text/plain" autocomplete="on">
                        <input id="search" name="q" type="text" placeholder="What are we looking for?">
                        <input id="search_submit" value="" type="submit"><span class="icon fa-search"></span></input>
                    </form>
                </div>
            </header>
        {% endblock header %}

        {% block banner %}
        {% endblock banner %}

        {% block main %}
        {% endblock main %}

        {% block cta %}
        {% endblock cta %}

        {% block footer %}
            {% include('_footer.twig') %}
        {% endblock footer %}

    </body>
    </html>
